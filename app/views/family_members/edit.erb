<title>Edit Family Member</title>
</head>
  <body>
  <h1>Edit Family Member</h1>
    <form action='/family_members/<%= @family_member.id %>' method='POST'>
      <input id='hidden' type='hidden' name='_method' value='patch'>
      <label>Name: </label>
      <input type='text' id='family_member_name' name='family_member[name]' value='<%=@family_member.name%>'>
      <input type='submit' value='Submit'>
    </form>
  </body>
</html>
